file = {:C\:/Users/Administrador/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Nath et al. - 2022 - Biodegradable methylcellulose biopolymer-derived activated porous carbon for dual energy application.pdf:pdf},
issn = {21941467},
journal = {Materials for Renewable and Sustainable Energy},
keywords = {Activated porous carbon,CH electrochemical workstation,Carbonization,Cyclovoltammetry,Electric double-layer capacitor,Methyl cellulose},
month = {dec},
number = {3},
pages = {241--250},
