pages = {567},
publisher = {Multidisciplinary Digital Publishing Institute},
title = {{Hydrogen Production via Electrolysis of Wastewater}},
url = {https://www.mdpi.com/2079-4991/14/7/567/htm https://www.mdpi.com/2079-4991/14/7/567},
volume = {14},
