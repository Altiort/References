doi = {10.1002/APP.51856},
file = {:C\:/Users/Administrador/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Jung, Sodano - 2022 - Synergetic effect of aramid nanofiber-graphene oxide hybrid filler on the properties of rubber compounds for tire.pdf:pdf},
