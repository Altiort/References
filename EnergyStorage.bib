file = {:C\:/Users/Administrador/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Bandaru et al. - 2023 - Improved mechanical properties of multi-layered PTFE composites through hybridisation.pdf:pdf},
issn = {0950-0618},
journal = {Construction and Building Materials},
keywords = {Flexural,Glass,Hybrid,Impact,Interlaminar shear,PTFE},
month = {apr},
pages = {130921},
publisher = {Elsevier},
title = {{Improved mechanical properties of multi-layered PTFE composites through hybridisation}},
volume = {374},
year = {2023}
}
@article{Barandun2019,
abstract = {We report an entirely new class of printed electrical gas sensors that are produced at near "zero cost". This technology exploits the intrinsic hygroscopic properties of cellulose fibers within paper; although it feels and looks dry, paper contains substantial amount of moisture, adsorbed from the environment, enabling the use of wet chemical methods for sensing without manually adding water to the substrate. The sensors exhibit high sensitivity to water-soluble gases (e.g., lower limit of detection for NH 3 < 200 parts-per-billion) with a fast and reversible response. The sensors show comparable or better performance (especially at high relative humidity) than most commercial ammonia sensors at a fraction of their price (<$0.02 per sensor). We demonstrate that the sensors proposed can be integrated into food packaging to monitor freshness (to reduce food waste and plastic pollution) or implemented into near-field-communication tags to function as wireless, battery-less gas sensors that can be interrogated with smartphones.},
author = {Barandun, Giandrin and Soprani, Matteo and Naficy, Sina and Grell, Max and Kasimatis, Michael and Kwan, † and Chiu, Lun and Ponzoni, Andrea and Gü, Firat},
doi = {10.1021/acssensors.9b00555},
file = {:C\:/Users/Administrador/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Barandun et al. - 2019 - Cellulose Fibers Enable Near-Zero-Cost Electrical Sensing of Water-Soluble Gases.pdf:pdf},
keywords = {RFID tags,Sensors,cellulose paper,food quality,waste prevention},
title = {{Cellulose Fibers Enable Near-Zero-Cost Electrical Sensing of Water-Soluble Gases}},
url = {https://pubs.acs.org/sharingguidelines},
year = {2019}
}
@article{Bayda2019,
abstract = {Nanoscience breakthroughs in almost every field of science and nanotechnologies make life easier in this era. Nanoscience and nanotechnology represent an expanding research area, which involves structures, devices, and systems with novel properties and functions due to the arrangement of their atoms on the 1–100 nm scale. The field was subject to a growing public awareness and controversy in the early 2000s, and in turn, the beginnings of commercial applications of nanotechnology. Nanotechnologies contribute to almost every field of science, including physics, materials science, chemistry, biology, computer science, and engineering. Notably, in recent years nanotechnologies have been applied to human health with promising results, especially in the field of cancer treatment. To understand the nature of nanotechnology, it is helpful to review the timeline of discoveries that brought us to the current understanding of this science. This review illustrates the progress and main principles of nanoscience and nanotechnology and represents the pre-modern as well as modern timeline era of discoveries and milestones in these fields.},
