issn = {19961944},
journal = {Materials},
keywords = {Additive manufacturing,Laser beam melting,Nitinol,Plasma electrolytic polishing},
month = {aug},
number = {15},
publisher = {MDPI AG},
title = {{Investigation of post-processing of additively manufactured nitinol smart springs with plasma-electrolytic polishing}},
volume = {14},
year = {2021}
}
