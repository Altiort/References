journal = {Frontiers in Built Environment},
keywords = {Bacteria-based concrete,Carbonate precipitation,Concrete permeability,Microbial self-healing,Nitrate reduction,Self-protected culture,X-ray computed tomography},
month = {nov},
pages = {404643},
publisher = {Frontiers Media S.A.},
