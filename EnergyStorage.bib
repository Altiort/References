issn = {13858947},
journal = {Chemical Engineering Journal},
keywords = {Carbon fiber,Cardanol,Composites,Epoxy vitrimer,Transesterification reaction},
number = {June},
pages = {144633},
publisher = {Elsevier B.V.},
title = {{Cardanol-based epoxy vitrimer/carbon fiber composites with integrated mechanical, self-healing, reprocessable, and welding properties and degradability}},
url = {https://doi.org/10.1016/j.cej.2023.144633},
volume = {471},
year = {2023}
}
