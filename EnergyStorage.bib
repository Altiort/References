keywords = {NiTi,Pseudoelasticity,Selective laser melting,Shape memory},
month = {may},
number = {5},
publisher = {MDPI AG},
title = {{Manufacturing and characterization of NiTi alloy with functional properties by selective laser melting}},
volume = {8},
year = {2018}
}
@article{Parker2009,
abstract = {Energy storage systems (ESSs) have been demonstrated to be useful assets to utilities and to others that generate, supply, or utilize electricity. Utilities, for example, have historically relied upon spinning reserve, occasionally provided by a neighboring utility, as a quickly available source of electric power to cope with a host of power-line anomalies. For example, a battery energy storage system (BESS), functioning as a rapid reserve resource, is an excellent substitute for spinning reserve in that its stored energy can be available to power utility loads in less time than power from a spinning reserve resource. Battery energy storage systems are also utilized as uninterruptible power supplies to support relatively isolated utility lines as well as dedicated loads. Other applications include storing energy from renewable, intermittent resources such as photovoltaics, wind-driven generators, and small hydro facilities. In these applications, the BESS provides an advantageous disconnect between resource availability and exploitation of the renewable energy. This article discusses the attributes of energy storage, describes a number of existing and past ESSs applications, and presents a brief description of a conceptual power inverter that converts direct current electricity from batteries, for example, to alternating current (ac) electricity. In many instances, the ac electricity is compatible with that distributed by utilities and, perhaps, may be synchronized and distributed with utility power. Nevertheless, conversions to other voltages and frequencies are also options. A number of historical and existing ESSs are described herein. Most of these are BESSs because they are, perhaps, more representative of the multiple ESS options than other choices and there are numerous BESS examples to cite.},
author = {Parker, C. D.},
doi = {10.1016/B978-044452745-5.00382-8},
file = {:C\:/Users/Administrador/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Parker - 2009 - APPLICATIONS – STATIONARY Energy Storage Systems Batteries.pdf:pdf},
isbn = {9780444527455},
journal = {Encyclopedia of Electrochemical Power Sources},
keywords = {Batteries,Electric-utility operations,Energy storage,Power conditioning},
month = {jan},
pages = {53--64},
publisher = {Elsevier},
title = {{APPLICATIONS – STATIONARY | Energy Storage Systems: Batteries}},
year = {2009}
}
@article{Parlett2016,
abstract = {The chemical functionality within porous architectures dictates their performance as heterogeneous catalysts; however, synthetic routes to control the spatial distribution of individual functions within porous solids are limited. Here we report the fabrication of spatially orthogonal bifunctional porous catalysts, through the stepwise template removal and chemical functionalization of an interconnected silica framework. Selective removal of polystyrene nanosphere templates from a lyotropic liquid crystal-templated silica sol-gel matrix, followed by extraction of the liquid crystal template, affords a hierarchical macroporous-mesoporous architecture. Decoupling of the individual template extractions allows independent functionalization of macropore and mesopore networks on the basis of chemical and/or size specificity. Spatial compartmentalization of, and directed molecular transport between, chemical functionalities affords control over the reaction sequence in catalytic cascades; herein illustrated by the Pd/Pt-catalysed oxidation of cinnamyl alcohol to cinnamic acid. We anticipate that our methodology will prompt further design of multifunctional materials comprising spatially compartmentalized functions.},
author = {Parlett, Christopher M.A. and Isaacs, Mark A. and Beaumont, Simon K. and Bingham, Laura M. and Hondow, Nicole S. and Wilson, Karen and Lee, Adam F.},
doi = {10.1038/NMAT4478},
file = {:C\:/Users/Administrador/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Parlett et al. - 2016 - Spatially orthogonal chemical functionalization of a hierarchical pore network for catalytic cascade reactions.pdf:pdf},
issn = {14764660},
journal = {Nature Materials},
month = {feb},
number = {2},
pages = {178--182},
pmid = {26569475},
publisher = {Nature Publishing Group},
title = {{Spatially orthogonal chemical functionalization of a hierarchical pore network for catalytic cascade reactions}},
volume = {15},
year = {2016}
}
