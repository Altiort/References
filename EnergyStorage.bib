issn = {13858947},
journal = {Chemical Engineering Journal},
keywords = {Biomass materials,Flexible devices,Pressure sensor,Remote control},
month = {sep},
publisher = {Elsevier B.V.},
title = {{Highly compressive aerogel-elastomer hybrid porous material with redistributed strain for high-sensitive soft pressure sensors}},
volume = {496},
year = {2024}
}
